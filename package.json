{
  "name": "@terminus/ui",
  "version": "0.0.0-PLACEHOLDER",
  "description": "Terminus UI Library",
  "license": "MIT",
  "author": "@terminus",
  "homepage": "https://github.com/GetTerminus/terminus-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/GetTerminus/terminus-ui.git"
  },
  "bugs": {
    "url": "https://github.com/GetTerminus/terminus-ui/issues"
  },
  "scripts": {
    "////////// Section: Library": "==============================",
    "library:build": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build library",
    "postlibrary:build": "yarn run library:build:styles && yarn run library:build:files && yarn run library:remove:prepublish",
    "library:build:watch": "yarn run library:build --watch",
    "library:build:prod": "yarn run library:build --prod",
    "library:build:styles": "npx gulp generate:styles --gulpfile tooling/gulpfile.js",
    "library:build:files": "cpr README.md dist/library/ && cpr LICENSE dist/library/",
    "library:lint": "yarn run library:lint:ts & yarn run library:lint:scss",
    "library:lint:fix": "yarn run library:lint:ts:fix & yarn run library:lint:scss:fix",
    "library:lint:ts": "npx eslint \"projects/library/**/*.{js,ts}\"",
    "library:lint:ts:fix": "yarn run library:lint:ts --fix",
    "library:lint:scss": "npx stylelint 'projects/library/**/!(*.spec).scss'",
    "library:lint:scss:fix": "yarn run library:lint:scss --fix",
    "library:test": "jest --config jest.library.config.js",
    "library:test:watch": "jest --config jest.library.config.js --watch",
    "library:test:ci": "yarn run library:test --runInBand --coverage",
    "library:test:ci:local": "yarn run library:test --coverage",
    "library:test:debug": "yarn run library:test --debug --runInBand",
    "library:remove:prepublish": "sed -i.bak '/prepublishOnly/d' dist/library/package.json && rm dist/library/package.json.bak",
    "//////// Section: Demo App": "==============================",
    "predemo:start": "yarn run library:build:styles",
    "demo:start": "ng serve demo",
    "demo:start:prod": "yarn run demo:start --prod",
    "demo:build": "ng build demo",
    "demo:build:prod": "yarn run demo:build --prod",
    "demo:build:ci": "yarn run demo:build:prod --progress false",
    "demo:lint": "yarn run demo:lint:ts & yarn run demo:lint:scss",
    "demo:lint:fix": "yarn run demo:lint:ts:fix & yarn run demo:lint:scss:fix",
    "demo:lint:ts": "npx eslint \"projects/demo/**/*.{js,ts}\"",
    "demo:lint:ts:fix": "yarn run demo:lint:ts --fix",
    "demo:lint:scss": "npx stylelint 'projects/demo/**/!(*.spec).scss'",
    "demo:lint:scss:fix": "yarn run demo:lint:scss --fix",
    "demo:test": "jest --config jest.demo.config.js",
    "demo:test:ci": "yarn run demo:test --runInBand --coverage",
    "demo:test:ci:local": "yarn run demo:test --coverage",
    "//////// Section: VisualRegression App": "==============================",
    "prevr:start": "yarn run library:build:styles",
    "vr:start": "ng serve visual-regression",
    "vr:start:prod": "yarn run vr:start --prod",
    "vr:build": "ng build visual-regression",
    "vr:build:prod": "yarn run vr:build:prod",
    "vr:build:ci": "yarn run vr:build:prod --progress false",
    "vr:lint": "yarn run vr:lint:ts & yarn run vr:lint:scss",
    "vr:lint:fix": "yarn run vr:lint:ts:fix & yarn run vr:lint:scss:fix",
    "vr:lint:ts": "npx eslint \"projects/visual-regression/**/*.{js,ts}\"",
    "vr:lint:ts:fix": "yarn run vr:lint:ts --fix",
    "vr:lint:scss": "npx stylelint 'projects/visual-regression/**/!(*.spec).scss'",
    "vr:lint:scss:fix": "yarn run vr:lint:scss --fix",
    "vr:test": "jest --config jest.visual-regression.config.js",
    "vr:test:ci": "yarn run vr:test --runInBand --coverage",
    "vr:test:ci:local": "yarn run vr:test --coverage",
    "////////// Section: Documentation": "==============================",
    "predocs": "yarn run docs:toc",
    "docs": "yarn run docs:ts",
    "docs:ci": "yarn run docs:ts --silent",
    "docs:ts": "npx compodoc",
    "docs:toc": "find . -path ./node_modules -prune -o -path ./dist -prune -o -path ./demo -prune -o -path ./.github -prune -o -path ./.git -prune -o -path ./CHANGELOG.md -prune -o -path ./CODE_OF_CONDUCT.md -prune -o -name '*.md' -print |  xargs npx doctoc --title '**Table of Contents**' --maxlevel 4",
    "////////// Section: Linting:ALL": "==============================",
    "lint:all": "yarn run library:lint & yarn run demo:lint & yarn run vr:lint",
    "lint:all:fix": "yarn run library:lint:fix & yarn run demo:lint:fix & yarn run vr:lint:fix",
    "////////// Section: Utility": "==============================",
    "replace-installed-library": "rm -rf node_modules/@terminus/ui && mkdir node_modules/@terminus/ui && cp -r dist/library/* node_modules/@terminus/ui",
    "ngcc": "ngcc --properties es2015 module main --first-only --async false",
    "delete-library-ngcc-files": "rm -rf node_modules/@terminus/ui/**/*.__ivy_ngcc_bak",
    "fresh-yarn-install": "rm -rf node_modules && yarn install",
    "link": "cd dist/library && yarn link && cd ../../ && yarn link @terminus/ui",
    "unlink": "yarn unlink @terminus/ui && cd dist/library && yarn unlink",
    "codecov:upload": "npx codecov -f coverage/*.json",
    "semantic-release": "semantic-release",
    "check:next-release": "npx semantic-release --no-ci --dry-run",
    "lint-staged": "lint-staged",
    "validate:circleci": "circleci config validate",
    "cm": "npx git-cz",
    "todo": "npx notes -x node_modules/ -x out-tsc/ -x dist/ -x coverage/ -x demo/ -x integration/ -x docs/ -x .DS_Store -x README.md -x projects/library/helpers.scss",
    "contributors:add": "npx all-contributors add",
    "contributors:generate": "npx all-contributors generate",
    "contributors:check": "npx all-contributors check"
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "lint-staged && yarn run docs:toc"
    }
  },
  "config": {
    "loglevel": "warn",
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./tooling/cz-config.js"
    }
  },
  "publishConfig": {
    "tag": "next"
  },
  "release": {
    "branch": "release",
    "plugins": [
      [
        "@semantic-release/release-notes-generator",
        {
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "subject",
              "scope"
            ]
          }
        }
      ],
      "@semantic-release/git"
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "CHANGELOG.md"
      },
      {
        "path": "@semantic-release/git",
        "message": "Add ${nextRelease.version} release notes [skip ci]"
      }
    ],
    "publish": [
      {
        "path": "@semantic-release/npm",
        "npmPublish": true,
        "pkgRoot": "dist/library"
      },
      {
        "path": "@semantic-release/github",
        "assets": [
          "CHANGELOG.md"
        ],
        "npmPublish": false
      }
    ]
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 5.0.0",
    "yarn": ">= 1.0.0"
  },
  "dependencies": {
    "@amcharts/amcharts4": "4.9.13",
    "@amcharts/amcharts4-geodata": "4.1.14",
    "@angular/animations": "9.1.0",
    "@angular/cdk": "9.2.0",
    "@angular/common": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.1.0",
    "@angular/material": "9.2.0",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/router": "9.1.0",
    "@terminus/design-tokens": "1.0.6",
    "@terminus/ngx-tools": "7.4.0",
    "@terminus/ui": "17.1.0",
    "date-fns": "2.11.1",
    "ngx-perfect-scrollbar": "9.0.0",
    "popper.js": "1.16.1",
    "text-mask-addons": "3.8.0",
    "text-mask-core": "5.1.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "9.0.1",
    "@angular-devkit/build-angular": "0.901.0",
    "@angular-devkit/build-ng-packagr": "0.901.0",
    "@angular-eslint/builder": "0.0.1-alpha.27",
    "@angular/cli": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@compodoc/compodoc": "1.1.11",
    "@ngrx/effects": "9.0.0",
    "@ngrx/store": "9.0.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/exec": "3.3.6",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/github": "5.4.3",
    "@semantic-release/npm": "5.1.15",
    "@semantic-release/release-notes-generator": "7.3.0",
    "@terminus/eslint-config-frontend": "^2.0.1",
    "@terminus/stylelint-config-frontend": "1.1.3",
    "@types/jest": "25.2.1",
    "@types/node": "13.11.0",
    "all-contributors-cli": "6.14.1",
    "autoprefixer": "9.7.5",
    "camelcase": "5.3.1",
    "chalk-cli": "4.1.0",
    "code-notes": "1.0.4",
    "commitizen": "4.0.3",
    "cpr": "3.0.1",
    "cz-customizable": "6.2.0",
    "del": "5.1.0",
    "doctoc": "1.4.0",
    "emojis": "1.0.10",
    "eslint": "6.8.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-deprecation": "1.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "22.1.0",
    "eslint-plugin-prefer-arrow": "1.1.7",
    "execa": "4.0.0",
    "glob": "7.1.6",
    "global": "4.4.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-cli": "2.2.0",
    "gulp-concat": "2.6.1",
    "gulp-postcss": "8.0.0",
    "gulp-print": "5.0.2",
    "gulp-remove-empty-lines": "0.1.0",
    "gulp-replace": "1.0.0",
    "gulp-sass": "4.0.2",
    "gulp-sourcemaps": "2.6.5",
    "gulp-strip-json-comments": "4.0.1",
    "husky": "4.2.3",
    "jest": "25.2.7",
    "jest-create-mock-instance": "1.1.0",
    "jest-junit": "10.0.0",
    "jest-preset-angular": "8.1.3",
    "jsdom": "16.2.2",
    "lint-staged": "10.1.2",
    "markdown-link-check": "3.8.0",
    "merge-files": "0.1.2",
    "micromatch": "4.0.2",
    "ng-packagr": "9.1.0",
    "node-sass": "4.13.1",
    "node-sass-import": "2.0.1",
    "remap-istanbul": "0.13.0",
    "require-dir": "1.2.0",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "sass-module-importer": "1.4.0",
    "sass-true": "5.0.0",
    "scssfmt": "1.0.7",
    "semantic-release": "15.13.24",
    "shell-quote": "^1.7.2",
    "stylelint": "13.3.0",
    "stylelint-order": "4.0.0",
    "systemjs": "6.2.6",
    "tsickle": "0.38.1",
    "tslib": "1.11.1",
    "typescript": "3.8.3",
    "validate-commit-msg": "2.14.0",
    "zone.js": "0.10.3"
  }
}

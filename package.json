{
  "name": "@terminus/ui",
  "version": "0.0.0",
  "description": "Terminus UI Library",
  "main": "./bundles/ui.umd.js",
  "module": "./ui.es5.js",
  "es2015": "./ui.js",
  "typings": "./ui.d.ts",
  "author": "@terminus",
  "license": "MIT",
  "repository": {},
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist \"src/lib/**/*.js\" \"src/lib/**/*.map\"",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "build-demo": "tsc -p src/demo/",
    "build-demo:watch": "tsc -p src/demo/ -w",
    "build-scss": "node-sass src/lib -o src/lib --output-style expanded",
    "build-scss:watch": "node-sass src/lib -o src/lib --output-style expanded -w",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "yarn run build-demo && yarn run build-scss",
    "start": "concurrently \"yarn run build-scss:watch\" \"yarn run build-demo:watch\" \"yarn run serve\"",
    "build-test": "tsc -p src/lib/tsconfig.spec.json",
    "build-test:watch": "tsc -p src/lib/tsconfig.spec.json -w",
    "pretest": "yarn run build-test",
    "test": "concurrently \"yarn run build-test:watch\" \"karma start karma.conf.js\"",
    "pretest:ci": "yarn run build-test",
    "test:ci": "karma start karma.conf.js --single-run --code-coverage",
    "preintegration": "yarn run build && cd integration && yarn run clean && yarn install",
    "integration": "yarn run integration:aot && yarn run integration:jit",
    "integration:jit": "cd integration && yarn run e2e",
    "integration:aot": "cd integration && yarn run e2e:aot",
    "lint:ts": "tslint ./src/**/*.ts -t verbose",
    "lint:scss": "sass-lint -c .sass-lint.yml 'src/lib/**/*.scss' -v -q",
    "lint": "yarn run lint:ts && yarn run lint:scss",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commitmsg": "validate-commit-msg",
    "notes": "notes -x node_modules/ -x .DS_Store -x dist/ -x coverage/ -x src/assets/",
    "cleanup:styleguide": "rm -rf styleguide",
    "docs:ts": "./node_modules/.bin/compodoc -p src/lib/tsconfig.compodoc.json -d docs --hideGenerator --toggleMenuItems all",
    "docs:scss": "node ./node_modules/.bin/nucleus --config config.nucleus.json",
    "docs": "yarn run docs:ts && yarn run docs:scss"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./ci-scripts/cz-config.js"
    }
  },
  "release": {
    "getLastRelease": "last-release-git",
    "verifyConditions": "condition-circle",
    "branch": "master"
  },
  "peerDependencies": {
    "@angular/core": ">=4.0.0 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/material": "2.0.0-beta.6",
    "angular2-ladda": "^1.2.1"
  },
  "devDependencies": {
    "@angular/animations": "^4.2.2",
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/compiler-cli": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "^4.2.2",
    "@angular/http": "^4.2.2",
    "@angular/material": "^2.0.0-beta.6",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "angular2-ladda": "^1.2.1",
    "camelcase": "^4.0.0",
    "codelyzer": "^3.0.1",
    "concurrently": "3.2.0",
    "core-js": "^2.4.1",
    "execa": "^0.7.0",
    "glob": "^7.1.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^3.0.3",
    "lite-server": "^2.2.2",
    "node-sass": "^4.5.2",
    "nucleus-styleguide": "^1.1.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.5",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs": "^5.0.1",
    "systemjs": "^0.19.40",
    "tslint": "^4.4.2",
    "typescript": "^2.2.1",
    "zone.js": "^0.8.4"
  }
}

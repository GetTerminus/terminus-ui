{
  "name": "@terminus/ui",
  "version": "0.0.0-development",
  "description": "Terminus UI Library",
  "main": "./bundles/ui.umd.js",
  "typings": "./ui.d.ts",
  "author": "@terminus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GetTerminus/terminus-ui.git"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "//=> Section: Documentation": "",
    "predocs": "yarn run docs:toc",
    "docs": "yarn run docs:ts && yarn run docs:scss",
    "docs:scss": "node ./node_modules/.bin/nucleus --config config.nucleus.json",
    "docs:ts": "./node_modules/.bin/compodoc -p src/lib/tsconfig.compodoc.json -d docs/typescript --hideGenerator --toggleMenuItems all --theme stripe",
    "docs:toc": "doctoc --title '**Table of Contents**' --maxlevel 4 ./README.md",
    "//=> Section: Development": "",
    "serve": "lite-server -c=tooling/bs-config.json --open false",
    "prestart": "yarn run build && yarn run build-demo && yarn run build-scss",
    "start": "concurrently \"yarn run build:watch\" \"yarn run build-demo:watch\"",
    "//=> Section: Build": "",
    "prebuild": "yarn run clean:lib",
    "build": "node tooling/build.js",
    "build:watch": "onchange 'src/lib/**/*.ts' 'src/lib/**/!(helpers).scss' 'src/lib/**/*.html' -- yarn run build",
    "build-demo": "cd src/demo && ng build --prod",
    "build-demo:watch": "cd src/demo && ng serve --port 4300 --preserve-symlink --progress false",
    "build-scss": "node-sass src/lib -o src/lib --output-style expanded",
    "build-scss:watch": "node-sass src/lib -o src/lib --output-style expanded -w",
    "build-test": "tsc -p src/lib/tsconfig.spec.json",
    "build-test:watch": "tsc -p src/lib/tsconfig.spec.json -w",
    "//=> Section: Cleanup": "",
    "clean:lib": "rimraf out-tsc dist \"src/lib/**/!(systemjs-angular-loader).js\" \"src/lib/**/*.map\" \"src/lib/helpers.scss\" \"src/lib/helpers.css\"",
    "clean:demo:node_modules": "cd src/demo rm -rf node_modules && yarn install",
    "clean:lib:node_modules": "rm -rf node_modules && yarn install",
    "//=> Section: NPM Management": "",
    "demo-npm-install": "cd src/demo && yarn install",
    "prefresh-npm-install": "rm -rf node_modules && cd src/demo && rm -rf node_modules",
    "fresh-npm-install": "yarn install && yarn run demo-npm-install",
    "postfresh-npm-install": "cd src/demo && yarn link @terminus/ui",
    "//=> Section: Integration": "",
    "preintegration": "yarn run build && cd integration && yarn run clean && yarn install",
    "integration": "yarn run integration:aot && yarn run integration:jit",
    "integration:jit": "cd integration && yarn run e2e",
    "integration:aot": "cd integration && yarn run e2e:aot",
    "//=> Section: Linting": "",
    "lint:ts": "./node_modules/.bin/tslint ./src/**/*.ts --format verbose",
    "lint:scss": "./node_modules/.bin/sass-lint -c .sass-lint.yml 'src/lib/**/*.scss' -v -q",
    "lint": "yarn run lint:ts && yarn run lint:scss",
    "markdown:test-links": "./node_modules/.bin/markdown-link-check ./README.md",
    "//=> Section: Release": "",
    "semantic-release-pre": "semantic-release pre",
    "semantic-release-post": "semantic-release post",
    "//=> Section: Setup": "",
    "setup": "node ./tooling/setup.js",
    "//=> Section: Testing": "",
    "pretest": "yarn run build-test",
    "test": "concurrently \"yarn run build-test:watch\" \"karma start karma.conf.js\"",
    "pretest:ci": "yarn run build-test",
    "test:ci": "karma start karma.conf.js --single-run --code-coverage --sauceLabs",
    "//=> Section: Tooling": "",
    "commitmsg": "validate-commit-msg",
    "cm": "./node_modules/.bin/git-cz",
    "todo": "./node_modules/.bin/notes -x node_modules/ -x out-tsc/ -x dist/ -x coverage/ -x src/demo/ -x integration/ -x docs/ -x .DS_Store -x README.md -x src/lib/helpers.scss",
    "update-scss-lib-extensions": "node tooling/replace-scss-lib-extension.js",
    "update-scss-demo-extensions": "node tooling/replace-scss-demo-extension.js",
    "watch-for-lib-css-extension": "onchange 'src/lib/**/*.js' -- yarn run update-scss-lib-extensions"
  },
  "config": {
    "loglevel": "verbose",
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./tooling/cz-config.js"
    }
  },
  "release": {
    "getLastRelease": "last-release-git",
    "verifyConditions": "condition-circle",
    "branch": "master"
  },
  "dependencies": {
    "@angular/animations": "4.4.6",
    "@angular/cdk": "2.0.0-beta.12",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6",
    "@angular/material": "2.0.0-beta.12",
    "@angular/platform-browser": "5.0.0",
    "@angular/router": "4.4.6",
    "angular2-ladda": "1.2.2"
  },
  "devDependencies": {
    "@angular/animations": "4.4.6",
    "@angular/cli": "1.4.9",
    "@angular/common": "4.4.6",
    "@angular/compiler": "4.4.6",
    "@angular/compiler-cli": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/flex-layout": "2.0.0-beta.9",
    "@angular/forms": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@angular/router": "4.4.6",
    "@compodoc/compodoc": "1.0.3",
    "@types/jasmine": "2.6.2",
    "@types/node": "8.0.47",
    "camelcase": "4.1.0",
    "code-notes": "1.0.4",
    "codelyzer": "3.2.2",
    "commitizen": "2.9.6",
    "concurrently": "3.5.0",
    "condition-circle": "1.5.0",
    "core-js": "2.5.1",
    "cz-customizable": "5.2.0",
    "doctoc": "1.3.0",
    "execa": "0.8.0",
    "glob": "7.1.2",
    "hammerjs": "2.0.8",
    "husky": "0.14.3",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.5",
    "karma-remap-coverage": "0.1.4",
    "karma-sauce-launcher": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-typescript": "3.0.7",
    "last-release-git": "0.0.3",
    "lite-server": "2.3.0",
    "markdown-link-check": "3.1.4",
    "merge-files": "0.1.1",
    "node-sass": "4.5.3",
    "nucleus-styleguide": "1.1.1",
    "onchange": "3.2.1",
    "remap-istanbul": "0.9.5",
    "replace-in-file": "3.0.0-beta.2",
    "rimraf": "2.6.2",
    "rollup": "0.47.4",
    "rollup-plugin-absolute-module-fix": "0.0.2",
    "rollup-plugin-commonjs": "8.2.1",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-sourcemaps": "0.4.1",
    "rollup-plugin-uglify": "2.0.1",
    "rxjs": "5.5.2",
    "sass-lint": "1.12.1",
    "semantic-release": "8.2.0",
    "systemjs": "0.20.19",
    "tslint": "5.8.0",
    "typescript": "2.5.3",
    "validate-commit-msg": "2.14.0",
    "zone.js": "0.8.18"
  }
}

{
  "name": "@terminus/ui",
  "description": "The Terminus UI Library",
  "version": "0.0.0-PLACEHOLDER",
  "license": "MIT",
  "author": "@terminus",
  "homepage": "https://github.com/GetTerminus/terminus-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/GetTerminus/terminus-ui.git"
  },
  "bugs": {
    "url": "https://github.com/GetTerminus/terminus-ui/issues"
  },
  "publishConfig": {
    "tag": "next"
  },
  "release": {
    "branch": "release",
    "verifyConditions": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "getLastRelease": "@semantic-release/npm",
    "publish": [
      {
        "path": "@semantic-release/exec",
        "cmd": "./tooling/inject-version.sh ${nextRelease.version}"
      },
      {
        "path": "@semantic-release/npm",
        "npmPublish": true,
        "pkgRoot": "../dist/library"
      },
      {
        "path": "@semantic-release/github",
        "npmPublish": false
      }
    ]
  },
  "sideEffects": false,
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 5.0.0",
    "yarn": ">= 1.0.0"
  },
  "peerDependencies": {
    "@angular/animations": "^9.0.0 || ^12.0.0",
    "@angular/cdk": "^9.0.0 || ^12.0.0",
    "@angular/common": "^9.1.0 || ^12.0.0",
    "@angular/core": "^9.0.0 || ^12.0.0",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "^9.0.0 || ^12.0.0",
    "@angular/material": "^9.0.0 || ^12.0.0",
    "@angular/platform-browser": "^9.0.0 || ^12.0.0",
    "@angular/router": "^9.0.0 || ^12.0.0",
    "@terminus/ngx-tools": ">=8.0.0",
    "@ngrx/effects": "^9.0.0",
    "@ngrx/store": "^9.0.0",
    "date-fns": "^2.0.1",
    "ngx-perfect-scrollbar": "9.0.0",
    "rxjs": "^6.5.4",
    "text-mask-addons": "3.8.0",
    "text-mask-core": "5.1.2"
  },
  "optionalDependencies": {
    "@amcharts/amcharts4": "^4.9.0",
    "@amcharts/amcharts4-geodata": "^4.1.6",
    "popper.js": "^2.0.0"
  }
}

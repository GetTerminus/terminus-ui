{
  "name": "@terminus/ui",
  "description": "The Terminus UI Library",
  "version": "0.0.0-PLACEHOLDER",
  "license": "MIT",
  "author": "@terminus",
  "homepage": "https://github.com/GetTerminus/terminus-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/GetTerminus/terminus-ui.git"
  },
  "bugs": {
    "url": "https://github.com/GetTerminus/terminus-ui/issues"
  },
  "publishConfig": {
    "tag": "next"
  },
  "release": {
    "branch": "release",
    "verifyConditions": [
      "condition-circle",
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "getLastRelease": "@semantic-release/npm",
    "publish": [
      {
        "path": "@semantic-release/exec",
        "cmd": "./tooling/inject-version.sh ${nextRelease.version}"
      },
      {
        "path": "@semantic-release/npm",
        "npmPublish": true,
        "pkgRoot": "../dist/library"
      },
      {
        "path": "@semantic-release/github",
        "npmPublish": false
      }
    ]
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 5.0.0",
    "yarn": ">= 1.0.0"
  },
  "dependencies": {
    "ngx-perfect-scrollbar": "8.0.0",
    "text-mask-addons": "3.8.0",
    "text-mask-core": "5.1.2"
  },
  "peerDependencies": {
    "@angular/animations": "^8.0.1",
    "@angular/cdk": "^8.0.1 || ^9.0.0",
    "@angular/core": "^8.0.1",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "^8.0.1",
    "@angular/material": "^8.0.1 || ^9.0.0",
    "@angular/platform-browser": "^8.0.1",
    "@angular/router": "^8.0.1",
    "@terminus/ngx-tools": ">=7.0.0",
    "date-fns": "^2.0.1",
    "rxjs": "^6.5.2"
  },
  "optionalDependencies": {
    "@amcharts/amcharts4": "^4.5.3",
    "@amcharts/amcharts4-geodata": "^4.1.6"
  }
}
